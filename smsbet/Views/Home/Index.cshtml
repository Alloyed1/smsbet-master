@section Scripts{
    <script type="text/javascript">
        let matches = $('.disciplines_date');
        
        for(let i = 0; i < matches.length; i++){
             let time = $(matches[i]);
             time.text(time.text().replace(/\s+/g, ''));
        }


        $('.all_bookmakers_line').on('click', function (event) {
            event.preventDefault();

            let mass = $(this).parent().find("p").text().split(", ").splice(3, 12);

            let massBookmakers = ['Фонбет', 'Bwin', 'Лига ставок', 'Олимп', 'Пари матч', 'Bet City', '1xBet', 'Балт Бет', 'Tennisi Bet', 'Leon', 'Winline', 'Marathon Bet',];
            
            mass.forEach((el) => {
                let index = massBookmakers.indexOf(el) + 1;
                $(this).parent().parent().append('<li><img src="//smsbet.ru/img/bookmakers/' + index + '.png" style="max-height:14px;max-width:52px;" alt="картинка"></li>');
            });
            $(this).remove();
        })
    </script>
 }
